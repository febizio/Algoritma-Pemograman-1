#include <iostream>
#include <string>

using namespace std;

int main ()
{
	string nama;
	nama = "abdullah";
	int usia;
	usia = 64;
	float tinggi;
	tinggi = 173.64;
	string hobby;
	hobby = "main main aja";
	
	cout<<"nama:	"<<nama<<'\n';
	cout<<"usia:	"<<usia<<"tahun"<<'\n';
	cout<<"tinggi:	"<<tinggi<<'\n';
	cout<<"hobby :	"<<hobby<<'\n';
}
	
